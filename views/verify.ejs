                        <div class="4u 12u$(xsmall)">
                            <input type="text" id="codeInputVerify" placeholder="Verification Code" pattern=".*" required><br />
                        </div>
                    </div>
                        <div class="12u$ 12u$(xsmall)">
                            <input type="submit" value="Verify" class="fit special">
