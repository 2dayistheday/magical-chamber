<body class="landing">

<div id="page-wrapper">
    <!-- Header -->
    <header id="header">
        <h1 id="logo"><a href="/">Chamber</a></h1>
        <nav id="nav">
            <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/#one">My Chambers</a></li>
                <li><button id="signoutBtn" class="button">Sign Out</button></li>
            </ul>
        </nav>
    </header>

    <!-- Main -->
    <div id="main" class="wrapper style1">
        <div class="container">
            <header class="major">
                <h2><%chamber_name%></h2>
                <p>화상 대화 진행중</p>
            </header>
            <div class="row 150%">
                <div class="3u 12u$(medium)">
                    <!-- Sidebar -->
                    <section id="sidebar">
                        <section>
                            <h3>VIDEO</h3>
                            <a href="#" class="image fit"><img src="/images/img_rtc_1.png" alt="프로필 사진누락" /></a>
                            <a href="#" class="image fit"><img src="/images/img_rtc_2.png" alt="프로필 사진누락" /></a>
                            <a href="#" class="image fit"><img src="/images/img_rtc_3.png" alt="프로필 사진누락" /></a>
                        </section>
                    </section>
                </div>

                <div class="1u$ 12u$(medium)">
                    <!-- Content -->
                    <section id="">
                        <div class="col-md-3">
                            <p>
                            </p>
                        </div>
                    </section>
                </div>
                <div class="1u$ 12u$(medium)">
                    <!-- Content -->
                    <section id="">
                        <div class="col-md-3">
                            <p>
                            </p>
                        </div>
                    </section>
                </div>

                <div class="3u 12u$(medium)">
                    <!-- Right Sidebar -->
                    <section id="content">
                        <section>
                            <h3>CHAT</h3>
                            <div class="12u$">
                                <textarea name="message" id="message" placeholder="Enter your message" rows="6"></textarea>
                            </div>
                            <div class="12u$">
                                <ul class="actions">
                                    <li><input type="submit" value="Send Message" class="special" /></li>
                                </ul>
                            </div>
                        </section>
                    </section>
                </div>
            </div>
        </div>
    </div>

    <!-- Footer -->
    <footer id="footer">
        <ul class="icons">
            <li><a href="#" class="icon alt fa-twitter"><span class="label">Twitter</span></a></li>
            <li><a href="#" class="icon alt fa-facebook"><span class="label">Facebook</span></a></li>
            <li><a href="#" class="icon alt fa-linkedin"><span class="label">LinkedIn</span></a></li>
            <li><a href="#" class="icon alt fa-instagram"><span class="label">Instagram</span></a></li>
            <li><a href="#" class="icon alt fa-github"><span class="label">GitHub</span></a></li>
            <li><a href="#" class="icon alt fa-envelope"><span class="label">Email</span></a></li>
        </ul>
        <ul class="copyright">
            <li>&copy; Redpoint. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
        </ul>
    </footer>
</div>

<!-- Scripts -->
<script src="/javascripts/js/jquery.min.js"></script>
<script src="/javascripts/js/jquery.scrolly.min.js"></script>
<script src="/javascripts/js/jquery.dropotron.min.js"></script>
